{
    "name": "nicko-ch/utils",
    "description": "my first package",
    "keywords": [
        "utils"
    ],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "NickoCh\\Utils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NickoCh\\Utils\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "nicko-ch",
            "email": "chenyaonan411@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": "^8.0",
        "jms/serializer": "^3.32",
        "psr/container": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.52",
        "symfony/console": "^6.4",
        "symfony/finder": "^6.4"
    }
}
